#SNAPSHOTS
###########################

BEGIN_SOUND_ATTACHMENT
	SNAPSHOT_NAME /in_mute
	EVENT_START_COND sim/graphics/view/view_is_external == 1
	EVENT_START_END sim/graphics/view/view_is_external == 0
END_SOUND_ATTACHMENT

BEGIN_SOUND_ATTACHMENT
	SNAPSHOT_NAME /init
	EVENT_START_COND sim/time/total_flight_time_sec > 0
	EVENT_END_COND sim/time/total_flight_time_sec > 5
END_SOUND_ATTACHMENT

BEGIN_SOUND_ATTACHMENT
	SNAPSHOT_NAME /flight_deck_door_cabin
	EVENT_START_COND sim/operation/sound/inside_ratio[0] == 0
	EVENT_END_COND sim/operation/sound/inside_ratio[0] != 0
END_SOUND_ATTACHMENT

BEGIN_SOUND_ATTACHMENT
	SNAPSHOT_NAME /lower_deck
	EVENT_ALWAYS
END_SOUND_ATTACHMENT

BEGIN_SOUND_ATTACHMENT
	SNAPSHOT_NAME /all
	EVENT_ALWAYS
END_SOUND_ATTACHMENT

BEGIN_SOUND_ATTACHMENT
	SNAPSHOT_NAME /old_vs_new_alarms
	EVENT_ALWAYS
END_SOUND_ATTACHMENT

